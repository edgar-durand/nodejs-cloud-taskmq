{
  "name": "cloudtaskmq-nodejs-example",
  "version": "1.0.0",
  "description": "CloudTaskMQ Complete Node.js API Example - Showcasing all library features",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
